<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="ContractListPagerIndicator">
        <attr name="txtFontSize" format="dimension"/>
        <attr name="txtNormalColor" format="color"/>
        <attr name="txtSelectColor" format="color"/>
        <attr name="visibleCount" format="integer" />
        <attr name="indicatorNormalColor" format="color"/>
        <attr name="indicatorSelectColor" format="color"/>
        <attr name="indicatorHeight" format="dimension"/>
        <attr name="indicatorMarginBottom" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="Header">
        <attr name="isShowDelete" format="boolean"/>
        <attr name="title" format="string"/>
    </declare-styleable>
    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"></attr>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="BottomButton">
        <attr name="bg" format="color"/>
        <attr name="txtColor" format="color"/>
        <attr name="txtSize"  format="dimension"/>
    </declare-styleable>
</resources>